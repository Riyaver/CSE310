import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Solution 
{

    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        int n1;
        n1 = sc.nextInt();
        int[] a1= new int[n1];
        
        int add = 0;
        
        for(int n=0;n<n1;n++)
        {
            a1[n]=sc.nextInt();
            add = add + a1[n];
        }
        System.out.println(add);
    }
}
