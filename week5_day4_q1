import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void factorial(int n)
    {
        int fact=1;
        for(int i=1;i<=n;i++)
        {    
            fact=fact*i;    
        } 
        System.out.print(fact);
    }
    public static void main(String[] args) {
        Scanner inp = new Scanner(System.in);
        int n = inp.nextInt();
        factorial(n);
    }
}
